<h1>New Song:</hi>
<%= form_for(@song) do |t|%>
    <label>Title: </label>
    <%= t.text_field :title %>
    <label>Released: </label>
    <%= t.check_box :released %>
    <label>Release Year: </label>
    <%= t.number_field :release_year %>
    <label>Artist Name: </label>
    <%= t.text_field :artist_name %>
    <label>Genre: </label>
    <%= t.text_field :genre %>
    <%= t.submit "Create Song"%>
<%end%>
